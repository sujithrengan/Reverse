<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.foxhunt_pragyan14" platformBuildVersionCode="19" platformBuildVersionName="4.4.2-1456859">
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <application android:allowBackup="true" android:debuggable="true" android:icon="@drawable/ic_launcher" android:label="FoxHunt 15" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.example.foxhunt_pragyan14.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_wifiscan" android:name="com.example.foxhunt_pragyan14.Wifiscan" android:parentActivityName="com.example.foxhunt_pragyan14.MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="com.example.foxhunt_pragyan14.MainActivity"/>
        </activity>
        <activity android:label="@string/title_activity_lock" android:name="com.example.foxhunt_pragyan14.Lock">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>
</manifest>
